{
  "compilerOptions": {
    "strict": true,
    "sourceMap": true,
    "module": "esnext",
    "target": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "jsx": "react",
    "moduleResolution": "node",
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "useDefineForClassFields": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "types": ["vite/client"]
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "custom.d.ts"
  ]
}