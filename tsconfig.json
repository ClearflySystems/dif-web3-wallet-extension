{
  "compilerOptions": {
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "strict": true,
    "sourceMap": true,
    "target": "esnext",
    "types": ["vite/client"],
    "isolatedModules": true
  },
  "exclude": [
    "node_modules"
  ]
}